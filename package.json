{
  "name": "vscode-lsaf-tools",
  "displayName": "vscode-lsaf-tools",
  "description": "A collection of tools for working with LSAF files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-lsaf-tools.getXAuthToken",
        "title": "Rest API: Get Auth Token",
        "category": "LSAF"
      },
      {
        "command": "vscode-lsaf-tools.deleteCredentials",
        "title": "Rest API: Delete Credentials",
        "category": "LSAF"
      },
      {
        "command": "vscode-lsaf-tools.lsaf-helloWorld",
        "title": "LSAF: Hello World"
      }
    ]
  },
  "scripts": {
    "build": "webpack",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "start": "code --extensionDevelopmentPath=.",
    "update": "npx npm-check-updates -u",
    "publish": "npx vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.97.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.20.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "axios-cookiejar-support": "^5.0.5",
    "js-beautify": "^1.15.2"
  }
}
